{
  "name": "ninja-prep",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "cross-env MOCHA_TEST=true mocha -r ts-node/register  tst/src/**/*.test.ts",
    "integration-test": "cross-env MOCHA_TEST=true mocha -r ts-node/register  tst/docker/**/*.test.ts",
    "server": "nodemon --verbose --config nodemon.json ./server.ts",
    "client": "cd ../client && yarn start",
    "lint": "eslint --ext js,jsx,ts,tsx src/",
    "prod": "cross-env NODE_ENV=production node build/server.js",
    "dev": "doppler run -- cross-env NODE_ENV=development concurrently \"yarn run server\" \"yarn run client\""
  },
