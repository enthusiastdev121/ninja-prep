    "husky": "^4.3.6",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:node/recommended"
    ],
    "rules": {
      "func-names": "off",
      "no-process-exit": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "object-shorthand": "off",
      "class-methods-use-this": "off"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
