    "express-list-endpoints": "^5.0.0",
    "express-session": "^1.17.1",
    "got": "^11.8.2",
    "jdoodle-client": "^1.0.2",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.16",
