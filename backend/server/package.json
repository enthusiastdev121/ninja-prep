{
  "name": "ninja-prep",
  "version": "1.0.0",
  "description": "",
  "main": "build/server/server.js",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "build": "ttsc --build tsconfig.json",
    "test": "cross-env MOCHA_TEST=true mocha -r ts-node/register -r tsconfig-paths/register tst/api/**/*.test.ts",
    "integration-test": "doppler run -- cross-env MOCHA_TEST=true mocha -r ts-node/register -r tsconfig-paths/register  tst/docker/**/*.test.ts",
    "server": "nodemon --verbose --config nodemon.json ./server.ts",
    "client": "cd ../../client && yarn start",
    "lint": "eslint --ext js,jsx,ts,tsx src/",
    "prod": "cross-env NODE_ENV=production node build/server/server.js",
    "dev": "doppler run -- cross-env NODE_ENV=development concurrently \"yarn run server\" \"yarn run client\""
  },
